<?xml version="1.0"?>
<robot name="obstacle_block">
    <link name="block">
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <box size="0.1 0.1 2.5"/>  <!-- Modify dimensions as needed -->
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>  <!-- Red color (RGBA: Red, Green, Blue, Alpha) -->
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0"/>
            <geometry>
                <box size="0.1 0.1 2.5"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="2.0"/>  <!-- Adjust mass as needed -->
            <origin xyz="0 0 0"/>
            <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
        </inertial>
    </link>
</robot>

